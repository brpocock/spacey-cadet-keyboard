! BRFP Space Cadet style layouts

clear Shift
clear Lock
clear Control
clear Mod1
clear Mod2
clear Mod3
clear Mod4
clear Mod5


! First, let's just alter the modifier and AID keys, without changing
! any of the typewriter keys' layouts.

! Tab on Delete key
keycode 22 = Tab ISO_Left_Tab

! Pause/Break key where Tab would usually go
! FIXME? I want Control+Pause = Break
keycode  23 = XF86Stop Pause XF86Stop Pause Break

! Left Control key doesn't move
keycode  37 = Control_L

keycode  62 = Shift_R 
keycode  64 = Meta_L 

! Backspace on “Caps Lock”
keycode  66 = BackSpace

! Help key on PF1
keycode  67 = Help F1 Help F1 Help F1 XF86Switch_VT_1 F1 F1 XF86Switch_VT_1 F1 F1 F1 F1 XF86Switch_VT_1
! Properties key on PF2
keycode  68 = SunProps F2 XF86Switch_VT_2 F2 F2 XF86Switch_VT_2 F2 F2 F2 F2 XF86Switch_VT_2
! Back key on PF3
keycode  69 = XF86Back SunUndo Cancel F3 Cancel F3 XF86Switch_VT_3 F3 F3 XF86Switch_VT_3 F3 F3 F3 F3 XF86Switch_VT_3
! History key on PF4
keycode  70 = XF86History SunPaste XF86History F4 XF86History F4 XF86Switch_VT_4 F4 F4 XF86Switch_VT_4 F4 F4 F4 F4 XF86Switch_VT_4
! Build key on PF5
!!!! keycode  71 = XF86New SunCut  XF86New F5 XF86New F5 XF86Switch_VT_5 F5 F5 XF86Switch_VT_5 F5 F5 F5 F5 XF86Switch_VT_5

!!!! keycode 71 = a A b B c C d D e E f F g G h H

! View key on PF6
keycode  72 = XF86View SunCopy XF86View F6 XF86View F6 XF86Switch_VT_6 F6 F6 XF86Switch_VT_6 F6 F6 F6 F6 XF86Switch_VT_6
! Execute key on PF7
keycode  73 = Execute XF86Send Execute F7 Execute F7 XF86Switch_VT_7 F7 F7 XF86Switch_VT_7 F7 F7 F7 F7 XF86Switch_VT_7
! REPL key on PF8
keycode  74 = F8 F8 F8 F8 F8 F8 XF86Switch_VT_8 F8 F8 XF86Switch_VT_9 F8 F8 F8 F8 XF86Switch_VT_8
! Print key on PF9
keycode  75 = Print F9 Print F9 Print F9 XF86Switch_VT_9 F9 F9 XF86Switch_VT_9 F9 F9 F9 F9 XF86Switch_VT_9
! Menu key on PF10
keycode  76 = Menu XF86MenuKB F10 F10 F10 F10 XF86Switch_VT_10 F10 F10 XF86Switch_VT_10 F10 F10 F10 F10 XF86Switch_VT_10
! --- also could use: XF86MenuPB osfMenu osfMenuBar
! Zoom key on PF11
keycode  95 = XF86ZoomIn XF86ZoomOut F11 F11 F11 F11 XF86Switch_VT_11 F11 F11 XF86Switch_VT_11 F11 F11 F11 F11 XF86Switch_VT_11
! Secure key on PF12
keycode  96 = XF86ScreenSaver XF86Away F12 F12 F12 F12 XF86Switch_VT_12 F12 F12 XF86Switch_VT_12 F12 F12 F12 F12 XF86Switch_VT_12
! Control+96 = XF86LogOff?


! Media play/pause on Print Screen/Sys Req key
keycode 107 = XF86AudioPlay XF86AudioPause
! += Control/Meta = XF86AudioPrev XF86AudioNext

! System Request key on Scroll Lock
keycode  78 = Sys_Req Sys_Req Sys_Req Sys_Req NoSymbol NoSymbol Terminate_Server NoSymbol NoSymbol Terminate_Server Sys_Req Sys_Req NoSymbol NoSymbol Terminate_Server

! Mute key on Pause/Break
keycode 122 = XF86AudioMute 
keycode 127 = XF86AudioMute 
! XF86AudioLowerVolume XF86AudioRaiseVolume


! AltGr key
keycode 133 = Mode_switch


! Delete on the lower-right corner Control key
keycode 105 = Delete

! Right Meta key doesn't move
keycode 108 = Meta_R
! Non-existent key?
keycode 109 = Linefeed

! Home on key labeled Insert
keycode 118 = Home 
! End on key labeled Home
keycode 110 = End 

! Prior on PageUp
keycode 112 = Prior 
! Next on PageDown
keycode 117 = Next 

! Hyper on Delete key
keycode 119 =  Hyper_R
! Insert on End key
keycode 115 = Insert

! Up on T-pad
keycode 111 = Up
! Left on T-pad
keycode 113 = Left
! Right on T-pad
keycode 114 = Right
! Down on T-pad
keycode 116 = Down 

! Compose on right win-key
keycode 134 = Multi_key
keycode  92 = Multi_key

! Control on ‘menu’ key
keycode 135 = Control_R 

! Left shift doesn't move
keycode  50 = Shift_L 

! Any key
keycode 94 = ISO_Level5_Shift

! OK, let's pretend we might not want to Dvorak things, but we do like
!redefining punctuation a little bit. Here's the compromise.

keycode  36 = Return


! Super key where Escape would usually go
keycode   9 = Super_L

! Escape on top-left key `~
keycode  49 = Escape


! Top row (after Escape)


! 1 ! ☠ ☿
keycode  10 = 1 exclam     U2620      U263f
! 2 | ∈ ♀
keycode  11 = 2 bar        elementof  femalesymbol
! 3 # ∋ ⊕
keycode  12 = 3 numbersign containsas U2295
! 4 $ ✗ ♂
keycode  13 = 4 dollar     U2717      malesymbol
! 5 % ♪ ♃
keycode  14 = 5 percent    U266a      U2643
! 6 _ † ♄
keycode  15 = 6 underscore dagger     U2644
! 7 & ∀ ♅
keycode  16 = 7 ampersand  U2200      U2645
! 8 ? □ ♆
keycode  17 = 8 question   U25a1      U2646
! 9 { ƒ ☽
keycode  18 = 9 braceleft  function   U263d
! 0 } ∞ ☉
keycode  19 = 0 braceright infinity   U2609
keycode  34 = parenleft  bracketleft
keycode  35 = parenright bracketright



keycode  48 = apostrophe quotedbl grave  notsign
keycode  59 = comma      less     comma  guillemotleft
keycode  60 = period     greater  period guillemotright

keycode  61 = slash backslash squareroot checkmark
! + = ✝ ≈
keycode  21 = plus equal U271d U2248
! - ~ — ^
keycode  20 = minus asciitilde emdash asciicircum 
! * @ ⁂ ★
keycode  51 = asterisk at U2042 U2605

! Clear key on Num Lock
keycode  77 = Clear KP_Delete U2422 U2423

! / ÷ λ ⁄ (fraction)
keycode 106 = KP_Divide U00f7 U03BB U2215 U2044 VoidSymbol XF86Ungrab 0x10000f7 VoidSymbol XF86Ungrab KP_Divide U2215 0x10000f7 VoidSymbol XF86Ungrab
! * × ° ⋅
keycode  63 = KP_Multiply U00d7 U00b0 U22C5 0x10000d7 VoidSymbol XF86ClearGrab 0x10000d7 VoidSymbol XF86ClearGrab KP_Multiply U22C5 0x10000d7 VoidSymbol XF86ClearGrab

keycode  79 = KP_7
keycode  80 = KP_8
keycode  81 = KP_9
! - ¯ (macron) ‐ (hyphen) – (en dash)
keycode  82 = KP_Subtract U00af U2010 endash VoidSymbol XF86Prev_VMode U2212 VoidSymbol XF86Prev_VMode KP_Subtract U2212 U2212 VoidSymbol XF86Prev_VMode

keycode  83 = KP_4
keycode  84 = KP_5
keycode  85 = KP_6
! + = ℵ •
keycode  86 = KP_Add KP_Equal U2135 U2022 0x100002b VoidSymbol XF86Next_VMode 0x100002b VoidSymbol XF86Next_VMode KP_Add 0x100002b 0x100002b VoidSymbol XF86Next_VMode

keycode  87 = KP_1
keycode  88 = KP_2
keycode  89 = KP_3
keycode 104 = KP_Enter

! also "00" "000" ??
keycode  90 = KP_0 
keycode  91 = KP_Decimal KP_Separator KP_Decimal KP_Separator




